<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="268.0" prefWidth="533.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.bytecoders.stock.controller.StockGUIController">
   <children>
      <ListView fx:id="lstAvailableBatches" layoutX="14.0" layoutY="37.0" prefHeight="184.0" prefWidth="232.0" />
      <ListView fx:id="lstSelectedBatches" layoutX="287.0" layoutY="37.0" prefHeight="184.0" prefWidth="232.0" />
      <Button fx:id="btnSelectBatch" layoutX="253.0" layoutY="94.0" mnemonicParsing="false" onAction="#selectBatch" text="&gt;" />
      <Button fx:id="btnDeselectBatch" layoutX="253.0" layoutY="132.0" mnemonicParsing="false" onAction="#deselectBatch" text="&lt;" />
      <Label layoutX="14.0" layoutY="14.0" text="Batches Available:" />
      <Label layoutX="290.0" layoutY="14.0" text="Batches in Stock:" />
      <Button fx:id="btnUpdateStock" layoutX="431.0" layoutY="230.0" mnemonicParsing="false" onAction="#updateStock" text="Update Stock" />
      <ProgressIndicator fx:id="progressIndicatorStock" layoutX="377.0" layoutY="103.0" />
      <ProgressIndicator fx:id="progressIndicatorAvailable" layoutX="104.0" layoutY="103.0" />
   </children>
</AnchorPane>
